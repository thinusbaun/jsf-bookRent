<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.katner.model.AuthPermission" table="auth_permission" schema="wypozyczalnia">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
        <property name="contentTypeId">
            <column name="content_type_id" sql-type="int(11)"/>
        </property>
        <property name="codename">
            <column name="codename" sql-type="varchar(100)" length="100"/>
        </property>
    </class>
</hibernate-mapping>