<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.katner.model.DjangoContentType" table="django_content_type" schema="wypozyczalnia">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="appLabel">
            <column name="app_label" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="model">
            <column name="model" sql-type="varchar(100)" length="100"/>
        </property>
    </class>
</hibernate-mapping>